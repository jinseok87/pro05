package com.myshop.web.controller.admin.board;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;
@Controller
@RequestMapping("/WEB-INF/view/admin/board/notice/")
public class NoticeController {
	@RequestMapping("list")
	public String list() {
		
	return "/WEB-INF/view/admin/board/notice/list.jsp";
	}
	@RequestMapping("detail")
	public String detail() {
		return "/admin/board/notice/derail.jsp";
	}
	@RequestMapping("reg")
	public String reg() {
		return "/admin/board/notice/reg.jsp";
	}
	
}
